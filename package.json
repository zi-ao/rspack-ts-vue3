{
  "name": "rspack-ts-vue3",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "postinstall": "simple-git-hooks",
    "build": "cross-env NODE_ENV=production rspack build",
    "check": "biome check --write",
    "dev": "cross-env NODE_ENV=development rspack serve",
    "format": "biome format --write"
  },
  "dependencies": {
    "@vicons/ionicons5": "^0.12.0",
    "@vueuse/core": "^11.2.0",
    "date-fns": "^4.1.0",
    "lodash": "^4.17.21",
    "naive-ui": "^2.40.1",
    "pinia": "^2.2.5",
    "vue": "^3.4.21",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.3",
    "@rspack/cli": "^1.0.14",
    "@rspack/core": "^1.0.14",
    "@types/lodash": "^4.17.13",
    "@types/node": "^22.8.7",
    "@unocss/postcss": "^0.63.6",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "dotenv-expand": "^11.0.6",
    "lint-staged": "^15.2.10",
    "postcss": "^8.4.47",
    "postcss-loader": "^8.1.1",
    "sass": "^1.80.6",
    "sass-embedded": "^1.80.6",
    "sass-loader": "^16.0.3",
    "simple-git-hooks": "^2.11.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3",
    "unocss": "^0.63.6",
    "unplugin-auto-import": "^0.18.3",
    "unplugin-vue-components": "^0.27.4",
    "vue-loader": "^17.3.1"
  },
  "lint-staged": {
    "*.{ts,ts,vue,js,jsx,css}": [
      "biome check --write"
    ]
  },
  "simple-git-hooks": {
    "pre-commit": [
      "npx lint-staged"
    ]
  }
}
